<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="spring.boot.hr.app.dao.DepartmentMapper">

    <select id="findAll" resultType="spring.boot.hr.app.model.Department">
        select
          id,
          depart_name as 'departName',
          depart_phone as'departPhone'
         from department
    </select>


</mapper>